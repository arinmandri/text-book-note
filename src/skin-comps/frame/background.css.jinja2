/* 배경 그림 */
#backImgBox .backImg {
  width: 100%;
  height: 100vh;
  position: fixed;
}
body:not(.theme-light) .backImg.light {
  display: none;
}
body:not(.theme-dark) .backImg.dark {
  display: none;
}
#backImgBox .backImgType_cover {
  background-position: center;
  background-size: cover;
}
#backImgBox .backImgType_stretch {
  background-size: 100vw 100vh;
}
#backImgBox .backImgType_VH {
  background-position: center;
  background-repeat: repeat;
}
#backImgBox .backImgType_HT {
  background-position: top;
  background-repeat: repeat-x;
}
#backImgBox .backImgType_HB {
  background-position: bottom;
  background-repeat: repeat-x;
}
#backImgBox .backImgType_VL {
  background-position: left;
  background-repeat: repeat-y;
}
#backImgBox .backImgType_VR {
  background-position: right;
  background-repeat: repeat-y;
}
#backImgBox .backImgType_X1 {
  background-position: top 0px left 0px;
  background-repeat: no-repeat;
}
#backImgBox .backImgType_X2 {
  background-position: top;
  background-repeat: no-repeat;
}
#backImgBox .backImgType_X3 {
  background-position: top 0px right 0px;
  background-repeat: no-repeat;
}
#backImgBox .backImgType_X4 {
  background-position: left;
  background-repeat: no-repeat;
}
#backImgBox .backImgType_X5 {
  background-position: center;
  background-repeat: no-repeat;
}
#backImgBox .backImgType_X6 {
  background-position: right;
  background-repeat: no-repeat;
}
#backImgBox .backImgType_X7 {
  background-position: top 0px right 0px;
  background-repeat: no-repeat;
}
#backImgBox .backImgType_X8 {
  background-position: bottom;
  background-repeat: no-repeat;
}
#backImgBox .backImgType_X9 {
  background-position: bottom 0px right 0px;
  background-repeat: no-repeat;
}