		<default><![CDATA[#333]]></default>
	</variable>
	<variable>
		<name>borderCol_dark</name>
		<label>금색</label>
		<description><![CDATA[기본값은 #444444]]></description>
		<type>COLOR</type>
		<default><![CDATA[#444444]]></default>
	</variable>
	<variable>
		<name>frameBack_dark</name>
		<label>딱지 밑색</label>
